{
	"name": "mern-movies",
	"version": "1.0.0",
	"description": "mern fullstack movies app",
	"type": "module",
	"main": "index.js",
	"scripts": {
		"fe": "cd frontend && npm run dev",
		"be": "nodemon backend/index.js",
