		"concurrently": "^8.2.2",
		"cookie-parser": "^1.4.6",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"joi": "^17.13.3",
		"jsonwebtoken": "^9.0.2",
		"multer": "^1.4.5-lts.1",
		"nodemon": "^3.1.4",
		"prisma": "^5.19.1"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.25.4",
		"babel-jest": "^29.7.0",
